public class TPC05 {
	public static void main(String[] args) {

		int[] a = new int[3]; // 배열 선언
		a[0] = 10;
		a[1] = 20;
		a[2] = 30;
		int sum=0;
		for(int i=0;i<a.length;i++) {
			sum +=a[i];
			System.out.println(sum + "0");
		}
		System.out.println(sum + "1");
		
		// 9개의 정수형 변수를 만들어라 -> 2차열 구조
	}

}
